using System;
using System.Globalization;

namespace primeiro
{
    class Program
    {
        static void Main(string[] args)
        {

            int N, a, b, c;
            N = int.Parse(Console.ReadLine());
            a = 1;
            b = 1;
            c = 1;
            for (int i = 0; i < N; i++)
            {
                Console.WriteLine(a + " " + b + " " + c );
                a += 1;
                b = a * a;
                c = a * b;
            }

        }
    }
}
