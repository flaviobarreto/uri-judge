using System;
using System.Globalization;

namespace Primeiro {
        class Program {
        static void Main(string[] args) {

            int A, B, C, D, Res;
            A = int.Parse(Console.ReadLine());
            B = int.Parse(Console.ReadLine());
            C = int.Parse(Console.ReadLine());
            D = int.Parse(Console.ReadLine());

            Res = A * B - C * D;
            
            Console.WriteLine("DIFERENCA = " + Res);

        }
    }
}
